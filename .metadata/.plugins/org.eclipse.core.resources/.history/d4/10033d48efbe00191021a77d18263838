package selection_sort;

public class SelectionSort {

	public static void main(String[] args) {

		int tmp = 0;
		for (int i=0; i < (arr.length()-1); i++) {
		  for (int j=1; j < (arr.length()-1); j++) {
		    tmp = 0;
		    if (arr[i] > arr[j]) {
		      tmp = arr[j];
		    }
		  }
		  arr[i] = tmp;
		}

	}

}
