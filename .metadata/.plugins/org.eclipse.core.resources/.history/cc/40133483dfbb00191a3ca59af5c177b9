package question1;

public class BubbleSort {
	
	static void bubbleSort(int arr[]) {
		int arrLen = arr.length;
	    int temp = 0;  
	     for(int i=0; i < arrLen; i++){  
	             for(int j=1; j < (arrLen-i); j++){  
	                      if(arr[j-1] > arr[j]){  
	                             //swap elements  
	                             temp = arr[j-1];  
	                             arr[j-1] = arr[j];  
	                             arr[j] = temp;  
	                     }  
	                      
	             }  
	     }
	}
	
	
	
}
