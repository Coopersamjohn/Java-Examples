package selection_sort;

public class SelectionSort {

	public static void main(String[] args) {

		int arr[]= {9,8,7,6,5,4,3,2,1};
		
		// selection sort
		int tmp = 0;
		for (int i=0; i < (arr.length); i++) {
		  for (int j=1; j < (arr.length-1); j++) {
		    tmp = 0;
		    if (arr[i] > arr[j]) {
		      tmp = arr[j];
		    }
		  }
		  arr[i] = tmp;
		}
		
		// print array
		for (int i = 0; i < arr.length; i++) {
			System.out.println(arr[i]);
		}

	}

}
